{{#contentFor "title"}}Athletic Team Building{{/contentFor}}
{{#contentFor "meta"}}
  <meta name="robots" content="noindex">
{{/contentFor}}

<div class="slanted-header first-thing">
  <div class="container">
    <h1>Athletic Team Building</h1>
	<h2>A Better Product On And Off The Field</h2>
    
  </div>
</div>

<div class="page-section page-section-light page-section-narrow first thing">
  <div class="container">

				{{#if enquirySubmitted}}
					<h2>Thank You</h2>
					<p>John looks forward to visiting with your team. We'll be in touch real soon. <strong>Hooyah!</strong></p>
					<p>&nbsp;</p>
					<p>&nbsp;</p>
					<p>&nbsp;</p>
					<p>&nbsp;</p>

				{{else}}
					<form method="post">
						<p class="lead">Request more information on our athletic team building program.</p>
						<br><br>
						<input type="hidden" name="action" value="contact">
						<div class="form-group {{#if validationErrors.name}}has-error{{/if}}">
							<label>Contact Name</label>
							<input type="text" name="name.full"  class="form-control" value="{{formData.[name.full]}}">
						</div>
						
						<div class="form-group {{#if validationErrors.email}}has-error{{/if}}">
							<label>Contact Email</label>
							<input type="email" name="email" class="form-control" value="{{formData.email}}">
						</div>
						<div class="form-group">
							<label>Contact Phone</label>
							<input type="text" name="phone" placeholder="(optional)" class="form-control" value="{{formData.phone}}">
						</div>

						<div class="form-group {{#if validationErrors.team}}has-error{{/if}}">
							<label>School/Team Name</label>
							<input type="text" name="team"  class="form-control" value="{{formData.team}}">
						</div>

						<div class="form-group {{#if validationErrors.location}}has-error{{/if}}">
							<label>Location of Training</label>
							<input type="text" name="location"  class="form-control" value="{{formData.location}}">
						</div>

						<div class="form-group {{#if validationErrors.dates}}has-error{{/if}}">
							<label>Proposed Date of Training</label>
							<input type="text" name="dates"  class="form-control" value="{{formData.dates}}">
						</div>

						<div class="form-group {{#if validationErrors.duration}}has-error{{/if}}">
							<label>Duration of Training</label>
							<input type="text" name="duration"  class="form-control" value="{{formData.duration}}">
						</div>

						<div class="form-group {{#if validationErrors.sport}}has-error{{/if}}">
							<label>Sport</label>
							<input type="text" name="sport"  class="form-control" value="{{formData.sport}}">
						</div>

						<div class="form-group {{#if validationErrors.division}}has-error{{/if}}">
							<label>Division</label>
							<input type="text" name="division"  class="form-control" value="{{formData.division}}">
						</div>

						<div class="form-group {{#if validationErrors.conference}}has-error{{/if}}">
							<label>League/Conference</label>
							<input type="text" name="conference"  class="form-control" value="{{formData.conference}}">
						</div>

						<div class="form-group {{#if validationErrors.participants}}has-error{{/if}}">
							<label>No. of Players or Participants</label>
							<input type="text" name="participants"  class="form-control" value="{{formData.participants}}">
						</div>

						<div class="form-group {{#if validationErrors.age}}has-error{{/if}}">
							<label>Age Range of Players</label>
							<input type="text" name="age"  class="form-control" value="{{formData.age}}">
						</div>

						<div class="form-group {{#if validationErrors.gender}}has-error{{/if}}">
							<label>Gender of Team</label>
							<input type="text" name="gender"  class="form-control" value="{{formData.gender}}">
						</div>

						<div class="form-group {{#if validationErrors.budget}}has-error{{/if}}">
							<label>Expected Budget</label>
							<input type="text" name="budget"  class="form-control" value="{{formData.budget}}">
						</div>

						<div class="form-group {{#if validationErrors.referral}}has-error{{/if}}">
							<label>How did You Hear About Us?</label>
							<input type="text" name="referral"  class="form-control" value="{{formData.referral}}">
						</div>

						<div class="form-group {{#if validationErrors.goals}}has-error{{/if}} ">
							<label>Training Goals</label>
							<textarea name="goals" placeholder="Tell us about your school or team..." rows="4" class="form-control">{{formData.goals}}</textarea>
						</div>
						<div class="form-actions">
							<button type="submit" class="btn btn-primary">Send</button>
						</div>
					</form>
				{{/if}}
			


  </div>
</div>


